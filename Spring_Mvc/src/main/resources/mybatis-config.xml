<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd" > <!-- mybatis dtd 문서정의 태그 -->

<configuration>
	<typeAliases>	
	    <!-- 이 파일의 역할은 데이터 저장빈 클래스 객체 별칭이름을 관리한다. -->
	    
	    <typeAlias type="net.daum.vo.MemberVO" alias="member"/>
	    <!-- MemberVO 빈클래스 객체 별칭이름을 member로 함. 이 member는 member.xml에서 참조한다. -->
	    
	    <typeAlias type="net.daum.vo.BoardVO" alias="board" />
	    	    
	</typeAliases>
</configuration>	